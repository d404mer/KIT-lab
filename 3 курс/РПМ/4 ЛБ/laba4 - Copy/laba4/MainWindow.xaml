<Window x:Class="laba4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:laba4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <!-- Стиль для Path -->
        <Style TargetType="Path">
            <Setter Property="Fill" Value="#FFFFFFFF"/>
            <!-- Цвет по умолчанию -->
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Fill" Value="#FF00FF00"/>
                    <!-- Цвет при наведении -->
                </Trigger>
            </Style.Triggers>
        </Style>


        <Storyboard x:Key="AnimateCanvas">
            <!-- Анимация для ScaleTransform -->
            <DoubleAnimation Storyboard.TargetName="scaleTransform"
             Storyboard.TargetProperty="ScaleX"
             To="0.17" Duration="0:0:0.5" />
            <DoubleAnimation Storyboard.TargetName="scaleTransform"
             Storyboard.TargetProperty="ScaleY"
             To="0.18" Duration="0:0:0.5" />
            <!-- Анимация для TranslateTransform -->
            <DoubleAnimation Storyboard.TargetName="translateTransform"
             Storyboard.TargetProperty="X"
             To="450" Duration="0:0:0.5" />
            <!-- Увеличено значение для X -->
            <DoubleAnimation Storyboard.TargetName="translateTransform"
             Storyboard.TargetProperty="Y"
             To="-2" Duration="0:0:0.5" />

            <!-- Задержка перед возвратом к исходным значениям -->
            <DoubleAnimation Storyboard.TargetName="scaleTransform"
             Storyboard.TargetProperty="ScaleX"
             BeginTime="0:0:2" To="0.1353119" Duration="0:0:0.5" />
            <DoubleAnimation Storyboard.TargetName="scaleTransform"
             Storyboard.TargetProperty="ScaleY"
             BeginTime="0:0:2" To="0.14259529" Duration="0:0:0.5" />
            <DoubleAnimation Storyboard.TargetName="translateTransform"
             Storyboard.TargetProperty="X"
             BeginTime="0:0:2" To="2.0130769" Duration="0:0:0.5" />
            <DoubleAnimation Storyboard.TargetName="translateTransform"
             Storyboard.TargetProperty="Y"
             BeginTime="0:0:2" To="-1.0973287" Duration="0:0:0.5" />
        </Storyboard>












