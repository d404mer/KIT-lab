      Storyboard.TargetProperty="Y"
      BeginTime="0:0:2" To="-1.0973287" Duration="0:0:0.5" />
        </Storyboard>












        <Storyboard x:Key="AnimateImage">
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TransformGroup.Children)[1].(TranslateTransform.X)"
                             To="300" Duration="0:0:1"/>
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                             To="1.5" Duration="0:0:1"/>
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"
                             To="1.5" Duration="0:0:1"/>
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TransformGroup.Children)[1].(TranslateTransform.X)"
                             BeginTime="0:0:2" To="0" Duration="0:0:1"/>
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                             BeginTime="0:0:2" To="1" Duration="0:0:1"/>
            <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"
                             BeginTime="0:0:2" To="1" Duration="0:0:1"/>
        </Storyboard>


